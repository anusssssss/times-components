{
  "name": "@times-components/ssr",
  "main": "dist/author",
  "dev": "src/author",
  "version": "1.3.3",
  "scripts": {
    "cleanup-dist": "rm -rf dist",
    "bundle": "NODE_ENV=production webpack -p",
    "bundle:dev": "webpack --config=webpack.config.dev.js",
    "bundle:prod": "NODE_ENV=production webpack --config=webpack.config.prod.js -p",
    "bundle:profile": "webpack --config=webpack.config.prod.js --profile --json > dist/stats.json",
    "depcheck": "depcheck --ignores='depcheck,eslint,prettier,react-art,webpack*' --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "ssr",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.8.2",
    "depcheck": "0.6.9",
    "eslint": "4.9.0",
    "prettier": "1.14.3",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.4",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-flow-react-proptypes": "22.0.0",
    "babel-plugin-react-native-web": "0.6.1",
    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0"
  },
  "dependencies": {
    "@times-components/article": "5.5.2",
    "@times-components/author-profile": "4.1.2",
    "@times-components/context": "0.5.0",
    "@times-components/provider": "1.6.0",
    "@times-components/schema": "0.1.7",
    "@times-components/styleguide": "3.5.2",
    "@times-components/topic": "3.4.2",
    "apollo-cache-inmemory": "1.1.10",
    "apollo-client": "2.3.7",
    "apollo-link-http": "1.5.3",
    "apollo-link": "1.2.2",
    "apollo-link-error": "^1.0.7",
    "express": "4.16.3",
    "node-fetch": "2.2.0",
    "react": "16.5.2",
    "react-apollo": "2.1.4",
    "react-art": "16.5.2",
    "react-dom": "16.5.2",
    "react-native": "0.55.4",
    "react-native-web": "0.9.0",
    "shrink-ray": "0.1.3",
    "styled-components": "3.4.0"
  }
}
